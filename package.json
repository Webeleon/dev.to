{
  "name": "@webeleon/dev.to",
  "version": "0.2.5",
  "description": "dev.to API client",
  "public": true,
  "keywords": [
    "dev.to",
    "API"
  ],
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "patch": "npm run build && npm version patch && npm publish",
    "format": "prettier --write '**/*.ts'",
    "build": "tsc",
    "generate-doc": "typedoc",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Webeleon/dev.to"
  },
  "bugs": {
    "url": "https://github.com/Webeleon/dev.to/issues"
  },
  "contributors": [
    {
      "name": "Julien Prugne",
      "email": "julien@webeleon.dev"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/jest": "27.0.1",
    "@types/sinon": "10.0.2",
    "husky": "7.0.2",
    "jest": "27.2.2",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "sinon": "11.1.2",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typedoc": "0.21.9",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "query-string": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,js,css,md}": "npm run generate-doc & prettier --write"
  }
}
